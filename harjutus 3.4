failinimi = input("Palun sisestage failinimi: ")

fail = open(failinimi, encoding="UTF-8")

laulud = []
nr = 1

for rida in fail:
    laul = rida.strip()
    laulud.append(laul)
    print(nr, laul)
    nr += 1

fail.close()

valik = int(input("Valige laulu järjekorranumber:"))

print("Mängitav muusikapala on", laulud[valik - 1])